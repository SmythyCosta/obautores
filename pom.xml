	<name>demo</name>
	<description>Demo project for Spring Boot</description>

	<properties>
		<!-- <java.version>1.8</java.version> -->
		<java.version>12</java.version>
	</properties>

	<dependencies>
	
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter</artifactId>
		</dependency>
		
		<dependency>
